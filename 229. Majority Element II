class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {
        vector<int> res;
        int n=nums.size();
        int cand1;
        int cand2;
        int count1=0;
        int count2=0;
        for(int i=0;i<n;++i)
        {
            if(nums[i]==cand1)
                count1++;
            else if(nums[i]==cand2)
                count2++;
            else
            {
                if(count1&&count2)
                {
                    count1--;
                    count2--;
                }
                else if(count1)
                {
                    cand2=nums[i];
                    count2=1;
                }
                else
                {
                    cand1=nums[i];
                    count1=1;
                }
            }
        }
        vector<int> tmp;
        if(count1>0) tmp.push_back(cand1);
        if(count2>0) tmp.push_back(cand2);
        for(int i=0;i<tmp.size();++i)
        {
            int count=0;
            for(int j=0;j<n;++j)
            {
                if(nums[j]==tmp[i])
                    count++;
            }
            if(count>n/3)
                res.push_back(tmp[i]);
        }
        return res;
    }
};229. Majority Element II
